{
  "name": "vue-zn-ui",
  "version": "0.1.6",
  "private": false,
  "description": "基于Vue-cli3的UI组件库",
  "main": "lib/zn-ui.umd.min.js",
  "keywords": "zn-ui",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zoey-git/zn-ui.git"
  },
  "scripts": {
    "serve": "npm run lib && npm run build:theme && vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name zn-ui --dest lib packages/index.js",
    "build:theme": "node build/bin/gen-cssfile && gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "dist": "npm run clean && npm run lib && npm run build:theme"
  },
  "dependencies": {
    "core-js": "^3.4.4",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.3",
    "cp-cli": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
